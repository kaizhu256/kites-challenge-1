{
  "name": "kites-challenge-1"
, "version": "1.0.0-2"
, "description": "Parse natural language hours into a structured format."
, "main": "lib/index.js"
, "scripts": {
    "lint": "node_modules/.bin/jshint lib test"
  , "start": "node test_application.js --repl --serverPort 8170"
  , "test": "utility2=node_modules/utility2/utility2.js; $utility2 --npm-test-mode start --npm-test-script test_application.js  --timeout-default 8000 --timeout-exit 16000; $utility2 --coveralls; $utility2 --npm-test-mode end"
  }
, "dependencies": {
    "underscore": "1.5.2"
  }
, "devDependencies": {
    "mocha": "1.17.1"
  , "jshint": "2.3.0"
  , "utility2": "~2014.1.25"
  }
, "jshintConfig": {
    "eqeqeq": true
  , "indent": 2
  , "newcap": true
  , "undef": true
  , "strict": true
  , "maxlen": 360
  , "asi": true
  , "laxcomma": true
  , "globals": {
      "require": true
    , "module": true
    , "exports": true
    , "suite": true
    , "test": true
    , "window": true
    , "global": true
    }
  }
, "repository" : {
    "type" : "git"
  , "url" : "https://github.com/kites/kites-challenge-1"
  }
}
