{
  "name": "kites-challenge-1"
, "version": "1.0.0-2"
, "description": "Parse natural language hours into a structured format."
, "main": "kites_hours.js"
, "scripts": {
    "lint": "node_modules/.bin/jshint lib test"
  , "start": "node kites_test.js --repl --serverPort 9999"
  , "test": "utility2=node_modules/utility2/utility2.js; $utility2 --npm-test-mode start --npm-test-script kites_test.js  --timeout-default 8000 --timeout-exit 300000; $utility2 --coveralls; $utility2 --npm-test-mode end; ## grunt test"
  }
, "dependencies": {
    "underscore": "1.5.2"
  , "utility2": "~2014.1.26"
  }
, "devDependencies": {
    "grunt": "~0.4.0"
  , "grunt-cli": "~0.1.6"
  , "grunt-saucelabs": "~4.1.2"
  , "mocha": "1.17.1"
  , "jshint": "2.3.0"
  }
, "jshintConfig": {
    "eqeqeq": true
  , "indent": 2
  , "newcap": true
  , "undef": true
  , "strict": true
  , "maxlen": 360
  , "asi": true
  , "laxcomma": true
  , "globals": {
      "require": true
    , "module": true
    , "exports": true
    , "suite": true
    , "test": true
    , "window": true
    , "global": true
    }
  }
, "repository" : {
    "type" : "git"
  , "url" : "https://github.com/kites/kites-challenge-1"
  }
}
